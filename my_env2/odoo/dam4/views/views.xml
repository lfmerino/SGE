<!--

En este fichero hemos definido una vista llamada empresas.empresa_list, 
a la que se accede a través de un menú llamado empresas.menu_root y que s
e activa a través de un actuador llamado empresas.action_empresa_window.
-->


		<record id="dam4.dam4_list" model="ir.ui.view"> 
			<field name="name">Lista de empresas</field>
			<field name="model">res.partner</field> 
		        <field name="arch" type="xml"> 
		            <tree> 
		                    <-field name="name"/> 
		                    <field name="vat"/> 
		                    <field name="lang"/> 
		                    <field name="street"/> 
		                    <field name="zip"/> 
		                    <field name="city"/> 
		                    <field name="phone"/> 
		                    <field name="email"/> 
	                       	    <field name="website"/> 
		               </tree> 
		        </field> 
		    </record> 

		    <record model="ir.actions.act_window" id="dam4.action_dam4_window"> 
			<field name="name">Listado de empresas</field>
			<field name="res_model">dam4</field>
			<field name="view_mode">tree,form</field>
		    </record>

		<!--  Declaramos los menu --> 
		<menuitem id="dam4.menu_root" name="Empresas"/> 
		<menuitem id="dam4.menu_1" name="Mantenimiento" parent="dam4.menu_root"/> 
		<menuitem id="dam4.menu_dam4_list" name="Empresas" parent="dam4.menu_1" action="dam4.action_dam4_window"/> 
	</data>
