<?xml version="1.0" encoding="UTF-8"?> 
<odoo> 
<!-- Comentario en la Views --> 
     <record id="view_ej_luis_form" model="ir.ui.view"> 
        <field name="name">ej.luis.form</field> 
        <field name="model">ej.luis</field> 
        <field name="arch" type="xml"> 
            <form string="Listado de Luis"> 
                <group> 
                    <field name="Nombre"/> 
                    <field name="Efdgfgdad"/> 
                </group> 
            </form> 
        </field> 
    </record> 
     <record id="view_tree_luis" model="ir.ui.view"> 
        <field name="name">ej.luis.tree</field> 
        <field name="model">ej.luis</field> 
        <field name="arch" type="xml"> 
           <tree> 
                    <field name="Nombre"/> 
                    <field name="Edad"/> 
           </tree> 
        </field> 
    </record> 
    <record model="ir.actions.act_window" id="act_ej_luis"> 
        <field name="name">luis</field> 
        <field name="res_model">ej.luis</field> 
        <field name="view_mode">tree,form</field> 
        <field name="view_id"  ref="view_tree_luis" /> 
    </record> 
<!--  Declaramos los menu --> 
<menuitem id="ej_luis_menu" name="Luis" web_icon="stock,static/description/icon.png" sequence="10"/> 
<menuitem id="submenu_ej_luis_menu" name="Luis" sequence="10" parent="ej_luis_menu"/> 
<menuitem id="submenu_ej_luis_action" name="Luis" sequence="10" parent="submenu_ej_luis_menu" action="act_ej_luis"/> 
</odoo> 
